{
  "inbounds": [
    {
      "type": "shadowsocks",
      "tag": "ss-in",
      "listen": "127.0.0.1",
      "listen_port": 1080,
      "method": "2022-blake3-aes-256-gcm",
      "password": "${SS_PASSWORD}",
      "multiplex": {
        "enabled": true
      },
      "transport": {
        "type": "ws",
        "path": "/ws"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ]
}
